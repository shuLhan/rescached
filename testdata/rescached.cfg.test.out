url = http://someurl

[dns "server"]
listen = 127.0.0.1:5350
tls.certificate =
tls.private_key =
parent = udp://10.8.0.1
http.idle_timeout = 0s
cache.prune_delay = 0s
cache.prune_threshold = 0s
