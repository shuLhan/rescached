[rescached]
file.resolvconf =
wui.listen = 127.0.0.1:5381
debug = 1

[block.d "a.block"]
name = a.block
url = http://127.0.0.1:11180/hosts/a

[block.d "b.block"]
name = b.block
url = http://127.0.0.1:11180/hosts/b

[block.d "c.block"]
name = c.block
url = http://127.0.0.1:11180/hosts/c

[block.d "test"]
